        });\r\n      }\r\n    },\r\n    methods: {\r\n      logout() {\r\n        this.$store\r\n          .dispatch(\"logout\")\r\n          .then(() => {\r\n            this.$router.push(\"/login\");\r\n          })\r\n          .catch((error) => {\r\n            console.log(error);\r\n          });\r\n      },\r\n      updateInitialState(token) {\r\n        this.$store.dispatch(\"getPlayerItems\", token);\r\n        this.$store.dispatch(\"getRoomItems\", token);\r\n      },\r\n    },\r\n    watch: {\r\n      // 若token發生變化就會查看\r\n      token() {\r\n        if (this.token) {\r\n          this.updateInitialState(this.token);\r\n        }\r\n      },\r\n    },\r\n  };\r\n</script>\r\n\r\n<style>\r\n  html,\r\n  body {\r\n    height: 100%;\r\n    background: #f2f6fa;\r\n  }\r\n\r\n  #app {\r\n    height: 100%;\r\n    display: flex;\r\n    align-items: center;\r\n    justify-content: center;\r\n  }\r\n\r\n  .container {\r\n    width: 100%;\r\n  }\r\n\r\n  .column--align-center {\r\n    margin: 0 auto;\r\n  }\r\n\r\n  .navigation-buttons {\r\n    position: absolute;\r\n    top: 5px;\r\n    z-index: 99;\r\n  }\r\n\r\n  .navigation-buttons .button {\r\n    margin: 0 2px;\r\n  }\r\n\r\n  .navigation-buttons .button .fa {\r\n    padding-right: 5px;\r\n  }\r\n\r\n  .a--logout {\r\n    background: #fff;\r\n    color: #000;\r\n    text-decoration: none;\r\n    text-transform: uppercase;\r\n    letter-spacing: 0.1;\r\n    font-weight: 400;\r\n    padding: 10px 30px;\r\n    transition: 0.5s;\r\n    left: 10px;\r\n  }\r\n\r\n  .a--logout:hover {\r\n    background: var(--clr);\r\n    color: white;\r\n    letter-spacing: 0.25em;\r\n  }\r\n\r\n  .a--logout:before {\r\n    content: \"\";\r\n    position: absolute;\r\n    inset: 2px;\r\n    background: #1a242a;\r\n  }\r\n  .a--logout span {\r\n    position: relative;\r\n    z-index: 1;\r\n    color: white;\r\n  }\r\n\r\n  .a--member {\r\n    background: #fff;\r\n    color: #000;\r\n    text-decoration: none;\r\n    text-transform: uppercase;\r\n    letter-spacing: 0.1;\r\n    font-weight: 400;\r\n    padding: 10px 30px;\r\n    transition: 0.5s;\r\n    left: 170px;\r\n  }\r\n\r\n  .a--member:hover {\r\n    background: var(--clr);\r\n    color: white;\r\n    letter-spacing: 0.25em;\r\n  }\r\n\r\n  .a--member:before {\r\n    content: \"\";\r\n    position: absolute;\r\n    inset: 2px;\r\n    background: #1a242a;\r\n  }\r\n  .a--member span {\r\n    position: relative;\r\n    z-index: 1;\r\n    color: white;\r\n  }\r\n</style>\r\n\r\n<!-- https://www.webdesigns.com.tw/css_align-center.asp 筆記 -->\r\n"],"sourceRoot":""}]}